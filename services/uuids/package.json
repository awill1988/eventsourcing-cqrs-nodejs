{
  "name": "users",
  "version": "0.0.1",
  "description": "Users Microservice",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run copy-static-assets && npm run lint && npm run build:ts",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.json",
    "build:ts": "babel src/ --out-dir build --extensions \".ts,.tsx,.js\" --source-maps inline --ignore \"**/*.ispec.ts,**/*.spec.ts,**/__tests__\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck --watch",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles --passWithNoTests --runInBand",
    "test:integration": "INTEGRATION_TESTS=1 DEBUG=true node_modules/.bin/jest --forceExit --runInBand --detectOpenHandles --coverage=$2 --testPathPattern=$1",
    "test:precommit": "npm run pretest && jest --forceExit --coverage --verbose --passWithNoTests --onlyChanged --detectOpenHandles",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:watch": "chokidar src/**/*.ts -c \"npm run lint\" --verbose --initial",
    "copy-static-assets": "node copyStaticAssets.js",
    "debug": "npm run watch:debug",
    "serve": "node --stack-size=65500 build/index.js",
    "serve:debug": "nodemon --signal SIGHUP --watch build --delay 1 --stack-size=65500 --inspect=0.0.0.0:${DEBUG_PORT:-6001} build/index.js",
    "start": "npm run watch",
    "watch": "concurrently -k -i --default-input-target 2 -p \"[{name}]\" -n \"ESLint,Babel,Server\" -c \"yellow.bold,blue.bold,cyan.bold,green.bold,white.bold\" \"npm run lint:watch\" \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:debug": "npm run prestart && concurrently -k -i --default-input-target 2 -p \"[{name}]\" -n \"ESLint,Babel,Server\" -c \"yellow.bold,blue.bold,cyan.bold,green.bold,white.bold\" \"npm run lint:watch\" \"npm run watch:ts\" \"npm run serve:debug\"",
    "watch:node": "nodemon --signal SIGHUP --watch build --delay 1 build/index.js",
    "watch:test": "npm run test -- --watchAll",
    "watch:ts": "babel src/ --watch --out-dir build --extensions \".ts,.tsx\" --source-maps inline --ignore \"**/*.ispec.ts,**/*.spec.ts,**/__tests__\" --verbose",
    "clean": "rimraf build && mkdir build && npm run lint",
    "prestart": "npm run clear-ports && npm run pretest && npm run clean && npm run build",
    "pretest": "npm run typecheck && npm run lint",
    "clear-ports": "lsof -ti tcp:${APP_PORT:-6000} | xargs kill"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "async": "^3.2.0",
    "bluebird": "^3.7.2",
    "bluebird-global": "^1.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "getmac": "^5.16.0",
    "source-map-support": "^0.5.16",
    "uuid": "8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/async": "^3.2.1",
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/concurrently": "^4.1.1",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.7",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.50",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-source-map-support": "^2.1.2",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-cli": "^25.5.4",
    "nodemon": "^2.0.2",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.9.7"
  },
  "author": "Adam Williams",
  "license": "ISC",
  "_moduleAliases": {
    "@src": "build/src"
  }
}
